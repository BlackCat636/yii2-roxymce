{
	"name" : "navatech/yii2-roxymce",
	"description" : "Integrate tinymce + roxy manager to yii2",
	"keywords" : [
		"yii2",
		"navatech",
		"tinymce",
		"file manager",
		"roxy",
		"fileman"
	],
	"type" : "yii2-extension",
	"license" : "MIT",
	"minimum-stability" : "stable",
	"support" : {
		"source" : "https://github.com/navatech/yii2-roxymce",
		"original" : "http://www.roxyfileman.com/"
	},
	"authors" : [
		{
			"name" : "Lyubomir Arsov",
			"homepage" : "http://www.roxyfileman.com"
		},
		{
			"name" : "Le Phuong",
			"email" : "phuong17889@gmail.com"
		}
	],
	"require" : {
		"php" : ">=5.4.0",
		"ext-gd" : "*",
		"ext-mbstring" : "*",
		"ext-zip" : "*",
		"navatech/yii2-navatech-base" : "@dev",
		"bower-asset/tinymce-dist" : "^4.3",
		"bower-asset/fontawesome" : "^4.0",
		"bower-asset/jquery-dateFormat" : "^1.0"
	},
	"autoload" : {
		"psr-4" : {
			"navatech\\roxymce\\" : "src"
		}
	}
